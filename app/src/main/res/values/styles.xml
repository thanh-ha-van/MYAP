<resources>
    <style name="AppBaseTheme" parent="Theme.Bar">

    </style>

    <style name="CustomDialog" parent="@android:style/Theme.Holo.Dialog">
        <item name="android:windowNoTitle">false</item>
    </style>
    AppBaseTheme
    <!-- Base application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="Theme.Bar" />

    <style name="AppTheme.PopupOverlay" parent="Theme.Bar" />

</resources>
